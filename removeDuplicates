/**
 * 核心思路： 定义快慢指针
 * 快指针表示遍历数组到达的下标位置，慢指针表示下一个不同元素要填入的下标位置
 */

public int removeDuplicates(int[] nums){
    int n = nums.length;
    if(n == 0){
      return 0;
    }
    int fast = 1,slow = 1;
    while(fast < n){
       if(nums[fast] != nums[fast - 1]){
           nums[slow] = nums[fast];
           ++slow;
       } 
    }
    return slow;
}
